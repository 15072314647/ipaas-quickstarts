{
  "apiVersion": "v1beta1",
  "kind": "Config",
  "id": "gogs-config",
  "items": [
    {
      "apiVersion": "v1beta1",
      "kind": "ReplicationController",
      "id": "gogs-rc",
      "desiredState": {
        "replicas": 1,
        "replicaSelector": {
          "component": "gogs"
        },
        "podTemplate": {
          "desiredState": {
            "manifest": {
              "version": "v1beta1",
              "name": "gogs-container",
              "containers": [
                {
                  "name": "gogs-image",
                  "image": "jimmidyson/gogs:0.5.13",
                  "imagePullPolicy": "PullIfNotPresent",
                  "ports": [
                    {
                      "name": "http",
                      "containerPort": 3000
                    },
                    {
                      "name": "ssh",
                      "containerPort": 22
                    }
                  ]
                }
              ]
            }
          },
          "labels": {
            "component": "gogs"
          }
        }
      },
      "labels": {
        "component": "gogs"
      }
    },
    {
      "apiVersion": "v1beta1",
      "kind": "Service",
      "id": "gogs-http-service",
      "containerPort": "http",
      "port": 3000,
      "selector": {
        "component": "gogs"
      }
    },
    {
      "apiVersion": "v1beta1",
      "kind": "Service",
      "id": "gogs-ssh-service",
      "containerPort": "ssh",
      "port": 22,
      "selector": {
        "component": "gogs"
      }
    },
    {
      "apiVersion": "v1beta1",
      "id": "gogs-route",
      "metadata": {
        "name": "gogs-route"
      },
      "kind": "Route",
      "host": "gogs.fabric8.local",
      "serviceName": "gogs-http-service"
    }
  ]
}
